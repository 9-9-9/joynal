
<h2>journal entry</h2>
<p><%= @entry.content %></p>
<h2>tags</h2>
<% @entry.tags.each do |tag| %>
  <%= link_to tag.name, tag_path(tag.name) %>
<% end %>

<%= form_for(:show_graph, url: show_graph_path, html: { class: 'show-graph' }, remote: true) do |f| %>
  <%= f.submit value: 'Show Graph' %>
<% end %>

<h3 id="journal-info"></h3>
<div class="d3">
</div>

