<div class="container">
  <div class="top-bar">
   <h2>Your Journal Entry on: <%= Date.parse(@entry.created_at.to_s) %></h2>
  </div>
  <div class="content"> 
    <p><%= @entry.content %></p>
  </div>
  <div class="tagged">
  <h2>tagged</h2>
    <% @entry.tags.each do |tag| %>
      <%= link_to tag.name, tag_path(tag.name) %>
    <% end %>
  </div>  
</div>





